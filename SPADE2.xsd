            <xs:element name="description" type="xs:string" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="configurationIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="workUnitsIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="milestoneIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="endDateIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="nameIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="descriptionIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="id" type="xs:int"/>
    </xs:complexType>

    <xs:complexType name="Criterion">
        <xs:sequence>
            <xs:element name="name" type="xs:string" minOccurs="0"
                        maxOccurs="unbounded" />
