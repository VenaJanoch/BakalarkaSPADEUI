            <xs:element name="workUnits" type="WorkUnit" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="workUnitIndexs" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="phases" type="Phase" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="iterations" type="Iteration" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="activities" type="Activity" minOccurs="0"
