            <xs:element name="nameIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="id" type="xs:int"/>
    </xs:complexType>

    <xs:complexType name="Commit">
        <xs:sequence>
            <xs:element name="cprs" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
