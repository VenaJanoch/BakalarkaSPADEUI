                        maxOccurs="unbounded" />
            <xs:element name="nameIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="descriptionIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="id" type="xs:int"/>
    </xs:complexType>


    <xs:complexType name="Iteration">
        <xs:sequence>
            <xs:element name="configuration" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="workUnits" type="WorkUnitList" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="name" type="xs:string" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="description" type="xs:string" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="startDate" type="xs:date" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="endDate" type="xs:date" minOccurs="0"
