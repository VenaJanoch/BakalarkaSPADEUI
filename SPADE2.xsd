            <xs:element name="endDateIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="nameIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="descriptionIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="configurationIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded" />
        </xs:sequence>
        <xs:attribute name="id" type="xs:int"/>
    </xs:complexType>

    <xs:complexType name="Activity">

        <xs:sequence>
            <xs:element name="workUnits" type="WorkUnitList" minOccurs="0"
