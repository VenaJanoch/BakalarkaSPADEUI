            <xs:element name="commitedDay" type="xs:date" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="name" type="xs:string" minOccurs="0"
                        maxOccurs="unbounded" />
            <xs:element name="commitedDayIndicator" type="xs:int" minOccurs="0"
