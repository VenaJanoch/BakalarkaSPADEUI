                        maxOccurs="unbounded"/>
            <xs:element name="severityIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="typeIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="resolutionIndicator" type="xs:int"
                        minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="statusIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="assigneeIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="authorIIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="relationIndicator" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>            
            <xs:element name="leftUnitIndex" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="rightUnitIndex" type="xs:int" minOccurs="0"
                        maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="exist" type="xs:boolean"/>
        <xs:attribute name="id" type="xs:int"/>
    </xs:complexType>


