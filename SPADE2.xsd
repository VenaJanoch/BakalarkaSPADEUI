                        maxOccurs="unbounded"/>
            <xs:element name="milestones" type="Milestone" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="criterions" type="Criterion" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="cpr" type="ConfigPersonRelation"
                        minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="configuration" type="Configuration"
                        minOccurs="0" maxOccurs="unbounded"/>
            <xs:element name="links" type="Link" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="priority" type="Priority" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="severity" type="Severity" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="status" type="Status" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="relation" type="Relation" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="resolution" type="Resolution" minOccurs="0"
                        maxOccurs="unbounded"/>
            <xs:element name="roleType" type="RoleType" minOccurs="0"
                        maxOccurs="unbounded"/>
