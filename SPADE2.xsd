                        maxOccurs="unbounded"/>
            <xs:element name="commitConfiguration" type="CommitedConfiguration" minOccurs="0"
                        maxOccurs="unbounded"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string"/>
        <xs:attribute name="description" type="xs:string"/>
        <xs:attribute name="endDate" type="xs:date"/>
        <xs:attribute name="startDate" type="xs:date"/>
    </xs:complexType>


    <xs:complexType name="WorkUnitList">
    <xs:sequence>
    <xs:element name="WorkUnits" type="xs:int" minOccurs="0"
