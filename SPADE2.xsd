        </xs:sequence>
        <xs:attribute name="id" type="xs:int"/>
    </xs:complexType>

    <xs:complexType name="CommitedConfiguration">
        <xs:sequence>
            <xs:element name="commitedDay" type="xs:date" minOccurs="0"
                        maxOccurs="unbounded"/>
